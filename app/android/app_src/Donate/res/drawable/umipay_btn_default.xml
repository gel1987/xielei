<?xml version="1.0" encoding="utf-8"?>
<!-- 按钮_白 -->
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="false">
        <shape>
            <corners android:radius="8dp" />
            <solid android:color="#f1f1f1" />
            <stroke
                android:width="0dp"
                android:color="#e8e8e8"
            />
        </shape>
    </item>
    <item android:state_pressed="true" android:drawable="@drawable/umipay_default_btn_pressed_bg">
    </item>
    <item android:state_enabled="true" android:drawable="@drawable/umipay_btn_white" /><!-- default -->
</selector>

